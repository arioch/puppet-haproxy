frontend <%= @name %>
	mode            <%= @mode %>
	bind            <%= @bind %>
<% @option.each do |option| -%>
	option          <%= option %>
<% end -%>
<% if @capture %>
  <% @capture.each do |capture| -%>
	capture         <%= capture %>
  <% end -%>
<% end -%>
<% if @redirect %>
  <% @redirect.each do |redirect| -%>
	redirect        <%= redirect %>
  <% end -%>
<% else -%>
	default_backend <%= @default_backend %>
<% end -%>


