frontend <%= @name %>
	mode            <%= @mode %>
	bind            <%= @bind %>
<% @option.each do |option| -%>
	option          <%= option %>
<% end -%>
<% if @capture %>
  <% @capture.each do |capture| -%>
	capture         <%= capture %>
  <% end -%>
<% end -%>
	default_backend <%= @default_backend %>

